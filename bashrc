alias dimmest=echo 20 > /sys/class/backlight/intel_backlight/brightness
alias dim=echo 100 > /sys/class/backlight/intel_backlight/brightness
alias normal=echo 2000 > /sys/class/backlight/intel_backlight/brightness
alias bright=echo 5000 > /sys/class/backlight/intel_backlight/brightness
alias brightest=echo 7812 > /sys/class/backlight/intel_backlight/brightness
